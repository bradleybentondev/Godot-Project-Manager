{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "godot_manager",
  "mainBinaryName": "godot_manager",
  "version": "0.8.0",
  "identifier": "com.paha.godot-manager",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "godot_manager",
        "width": 1400,
        "height": 600,
        "minWidth": 1400,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "scope": ["**"],
        "enable": true
      }
    }
  }
}